def_key "l"
  next_column
def_key "h"
  previous_column
def_key "k"
  scroll_up
def_key "j"
  scroll_down

def_key "."
  show_lyrics

def_key "t"
  select_item

def_key "ctrl-u"
  page_up
def_key "ctrl-d"
  page_down

def_key "d"
  delete_stored_playlist
def_key "d"
  delete_playlist_items

def_key "escape"
  jump_to_parent_directory
